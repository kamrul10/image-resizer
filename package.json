{
  "name": "image-resizer",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "clean": "rimraf dist/*",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "tsc": "tsc",
    "build": "npm-run-all clean lint tsc",
    "dev:start": "npm-run-all build start:server",
    "dev:worker": "npm-run-all build start:worker",
    "server": "nodemon --watch src -e ts --exec npm run dev:start",
    "worker": "nodemon --watch worker -e ts --exec npm run dev:worker",
    "start:server": "node dist/src/index.js",
    "start:worker": "node dist/worker/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
